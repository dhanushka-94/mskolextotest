APP_NAME="MSK COMPUTERS"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# PayHere Configuration
PAYHERE_MERCHANT_ID=your_merchant_id
PAYHERE_MERCHANT_SECRET=your_merchant_secret
PAYHERE_MODE=sandbox

# WebXPay Configuration
WEBXPAY_MODE=sandbox
WEBXPAY_API_USERNAME=your_api_username
WEBXPAY_API_PASSWORD=your_api_password
WEBXPAY_SECRET_KEY=your_secret_key
WEBXPAY_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----
YOUR_WEBXPAY_PUBLIC_KEY_HERE
-----END PUBLIC KEY-----"
WEBXPAY_CHECKOUT_URL="https://stagingxpay.info/index.php?route=checkout/billing"
WEBXPAY_RETURN_URL="${APP_URL}/payment/webxpay/return"
WEBXPAY_CANCEL_URL="${APP_URL}/payment/webxpay/cancel"
WEBXPAY_NOTIFY_URL="${APP_URL}/payment/webxpay/notify"

# Koko Pay Configuration
KOKOPAY_MODE=sandbox
KOKOPAY_MERCHANT_ID=your_kokopay_merchant_id
KOKOPAY_API_KEY=your_kokopay_api_key
KOKOPAY_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
YOUR_KOKOPAY_PRIVATE_KEY_HERE
-----END RSA PRIVATE KEY-----"
KOKOPAY_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----
YOUR_KOKOPAY_PUBLIC_KEY_HERE
-----END PUBLIC KEY-----"
KOKOPAY_PLUGIN_NAME="MSK_Computers"
KOKOPAY_PLUGIN_VERSION="1"
KOKOPAY_CURRENCY=LKR
KOKOPAY_RETURN_URL="${APP_URL}/payment/kokopay/return"
KOKOPAY_CANCEL_URL="${APP_URL}/payment/kokopay/cancel"
KOKOPAY_NOTIFY_URL="${APP_URL}/payment/kokopay/notify"

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Main Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=msk_computers
DB_USERNAME=root
DB_PASSWORD=

# Products Database (Read-Only) - Separate Credentials
PRODUCTS_DB_HOST=127.0.0.1
PRODUCTS_DB_PORT=3306
PRODUCTS_DB_DATABASE=msk_computers_products
PRODUCTS_DB_USERNAME=root
PRODUCTS_DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"